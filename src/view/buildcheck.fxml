<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="528.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView>
            <columns>
                <TableColumn text="Type" />
                <TableColumn text="Name" />
                <TableColumn text="Price" />
                <TableColumn />
            </columns>
        </TableView>
        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" textAlignment="RIGHT" VBox.vgrow="ALWAYS">Total:$0.00</Label>
        <Button styleClass="button" text="Add New Part to Catalogue">
        </Button>
        <Button styleClass="button" text="Remove Selected from Catalogue">
        </Button>
        <Button styleClass="button" text="Close" />
    </children>
</VBox>
